<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rpmtravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Dzikir_counter"></activity>
        <activity android:name=".Dzikir" />
        <activity android:name=".Alquran_detail" /> <!-- <activity android:name=".Masjid"></activity> -->
        <activity android:name=".MyDoa" />
        <activity android:name=".Doa" />
        <activity android:name=".Verifikasi" />
        <activity android:name=".EditProfile" />
        <activity android:name=".My_Program" />
        <activity android:name=".My_Profile" />
        <activity android:name=".SuccessBooking" />
        <activity android:name=".Program_Checkout" />
        <activity android:name=".Program_Detail" /> <!-- <activity android:name=".Home" /> -->
        <activity android:name=".SuccessRegister" />
        <activity android:name=".RegisterTwo" />
        <activity android:name=".RegisterOne" />
        <activity android:name=".SignIn" />
        <activity android:name=".GetStarted" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Masjid"
            android:label="@string/title_activity_masjid" />
        <activity android:name=".Home" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
    </application>

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" /> <!-- Required to show current location -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" /> <!-- Required OpenGL ES 2.0. for Maps V2 -->
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

</manifest>